; https://www.linux.com/topic/networking/how-set-private-dns-servers-bind-ubuntu-1604-1/
$TTL    60
@       IN      SOA     ns1.nope. admin.nope. (
                  52       ; Serial
             604800     ; Refresh
              86400     ; Retry
            2419200     ; Expire
             604800 )   ; Negative Cache TTL

; name servers - NS records
     IN      NS      ns1.nope.

; name servers - A records
ns1.nope.          IN      A       127.0.0.1

; 10.20.0.0/16 - A records - all from tailscale
pythia.nope.        IN      A      100.123.146.128
niobe.nope.        IN      A      100.90.204.62
morpheus.nope.        IN      A      100.116.45.26
chunky.nope.       IN       A      100.98.89.16
ancient-one.nope.        IN      A       100.126.171.104
franzk.nope.        IN      A       100.65.115.17

syncthing.franzk.nope. IN CNAME franzk.nope.
nextcloud.nope. IN CNAME franzk.nope.
grafana.nope. IN CNAME pythia.nope.
